pipeline {
    agent any
    }
    stages {
        stage("init") {
            steps {
                echo ‘checking out the application….’
            }
        }
        stage("build") {
            steps {
                echo ‘building the application….’
            }
        }
        stage("test") {
            steps {
                echo ‘testing the application….’
            }
        }
        stage("deploy") {
            steps {
                echo ‘deploying the application….’
            }
        }
    }   
}
